<?xml version="1.0" encoding="UTF-8"?>
<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- injection par setter -->
	<bean id="helloBean" class="com.zainab.Spring.SpringHello">
		<property name="username" value="My name is Zainab"></property>
	</bean>
	<bean id="helloBean2" class="com.zainab.Spring.SpringHello">
		<property name="username" value="My name is Sara"></property>
	</bean>
	<!-- ou injection par constructeur -->
	<bean id="helloBean3" class="com.zainab.Spring.SpringHello">
		<constructor-arg value="My name is What?" />	

	</bean>
	</beans>